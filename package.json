{
	"name": "vsc-webshark",
	"displayName": "vsc-webshark",
	"description": "pcap / network trace / wireshark extension",
	"version": "0.1.0",
	"publisher": "mbehr1",
	"license": "GPL-2.0",
	"author": {
		"name": "Matthias Behr",
		"email": "mbehr+vscwebshark@mcbehr.de"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/mbehr1/vsc-webshark.git"
	},
	"bugs": {
		"url": "https://github.com/mbehr1/vsc-webshark/issues"
	},
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onWebviewPanel:vsc-webshark",
		"onCommand:webshark.openFile"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "webshark.openFile",
				"title": "Open pcap file..."
			}
		],
		"configuration": [
			{
				"title": "VSC-Webshark",
				"properties": {
					"vsc-webshark.sharkdFullPath": {
						"type": "string",
						"default": "sharkd",
						"description": "Full path incl. file to the Wireshark - sharkd binary. e.g. for OSX: /Applications/Wireshark.app/Contents/MacOS/sharkd"
					}
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.44.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.18.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"vscode-extension-telemetry": "^0.1.5"
	}
}